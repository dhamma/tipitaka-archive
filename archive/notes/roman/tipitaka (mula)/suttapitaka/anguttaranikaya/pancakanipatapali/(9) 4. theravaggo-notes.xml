      <alternative source-abbr="kaṃ." source="kaṃ.">kupanīye</alternative>
      <alternative source-abbr="pī." source="pali text society">kopanīye</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> dussati, mohanīye muyhati, kuppanīye </snippet>
