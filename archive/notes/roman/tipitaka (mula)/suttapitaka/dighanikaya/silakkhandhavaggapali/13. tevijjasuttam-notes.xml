<notes archive-path="roman/tipitaka (mula)/suttapitaka/dighanikaya/silakkhandhavaggapali/13. tevijjasuttam-notes.xml">
  <note line="3" original="sattamācariyamahayugā (syā.)" reference-line="16" id="0">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">ācariyāmahayugā</alternative>
      <alternative source-abbr="syā." source="thai">sattamācariyamahayugā</alternative>
    </alternatives>
    <type>auto</type>
    <snippet>Kiṃ pana, vāseṭṭha, atthi koci tevijjānaṃ brāhmaṇānaṃ yāva sattamā ācariyāmahayugā </snippet>
  </note>
  <note line="12" original="samīhitaṃ (syā.)" reference-line="17" id="1">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">samihitaṃ</alternative>
      <alternative source-abbr="syā." source="thai">samīhitaṃ</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> Kiṃ pana, vāseṭṭha, yepi tevijjānaṃ brāhmaṇānaṃ pubbakā isayo mantānaṃ kattāro mantānaṃ pavattāro, yesamidaṃ etarahi tevijjā brāhmaṇā porāṇaṃ mantapadaṃ gītaṃ pavuttaṃ samihitaṃ </snippet>
  </note>
  <note line="21" original="teva (ka.)" reference-line="20" id="2">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">teva</alternative>
    </alternatives>
    <type>no_match</type>
    <snippet>Sādhu, vāseṭṭha, te vata </snippet>
  </note>
  <note line="29" original="kiṃ pana (sī. syā. pī.)" reference-line="26" id="3">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">pana</alternative>
      <alternative source-abbr="sī." source="sri lanka">kiṃ pana</alternative>
      <alternative source-abbr="syā." source="thai">kiṃ pana</alternative>
      <alternative source-abbr="pī." source="pali text society">kiṃ pana</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> Iti pana </snippet>
  </note>
  <note line="40" original="sattamehi (?)" reference-line="26" id="4">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">sattamā</alternative>
      <alternative source-abbr="?" source="unknown">sattamehi</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> ācariyapācariyehi brahmā sakkhidiṭṭho. Napi kira tevijjānaṃ brāhmaṇānaṃ yāva sattamā </snippet>
  </note>
  <note line="49" original="yaṃ tvaṃ (syā.)" reference-line="37" id="5">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">yassa tvaṃ</alternative>
      <alternative source-abbr="syā." source="thai">yaṃ tvaṃ</alternative>
    </alternatives>
    <type>auto</type>
    <snippet>, vāseṭṭha, puriso cātumahāpathe nisseṇiṃ kareyya – pāsādassa ārohaṇāya. Tamenaṃ evaṃ vadeyyuṃ – ambho purisa, yassa tvaṃ </snippet>
  </note>
  <note line="58" original="ophuṭā (sī. ka.), ophutā (syā.)" reference-line="55" id="6">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="sī." source="sri lanka">ophuṭā</alternative>
      <alternative source-abbr="ka." source="cambodian">ophuṭā</alternative>
      <alternative source-abbr="syā." source="thai">ophutā</alternative>
    </alternatives>
    <type>no_match</type>
    <snippet> kho, vāseṭṭha, pañcahi nīvaraṇehi tevijjā brāhmaṇā āvuṭā nivuṭā onaddhā </snippet>
  </note>
  <note line="68" original="pariyonaddhā, te (syā. ka.)" reference-line="55" id="7">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">pariyonaddhā</alternative>
      <alternative source-abbr="syā." source="thai">pariyonaddhā, te</alternative>
      <alternative source-abbr="ka." source="cambodian">pariyonaddhā, te</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> dhammā brāhmaṇakārakā te dhamme pahāya vattamānā, ye dhammā abrāhmaṇakārakā te dhamme samādāya vattamānā pañcahi nīvaraṇehi āvuṭā nivuṭā onaddhā pariyonaddhā </snippet>
  </note>
  <note line="78" original="ādisitvā (ka.)" reference-line="61" id="8">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">āsīditvā</alternative>
      <alternative source-abbr="ka." source="cambodian">ādisitvā</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> Idha kho pana te, vāseṭṭha, tevijjā brāhmaṇā āsīditvā </snippet>
  </note>
  <note line="87" original="visādaṃ (sī. pī.), visattaṃ (syā.)" reference-line="61" id="9">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">visāraṃ</alternative>
      <alternative source-abbr="sī." source="sri lanka">visādaṃ</alternative>
      <alternative source-abbr="pī." source="pali text society">visādaṃ</alternative>
      <alternative source-abbr="syā." source="thai">visattaṃ</alternative>
    </alternatives>
    <type>auto</type>
    <snippet> saṃsīdanti, saṃsīditvā visāraṃ </snippet>
  </note>
  <note line="98" original="sukkhataraṇaṃ (ka.)" reference-line="61" id="10">
    <extra></extra>
