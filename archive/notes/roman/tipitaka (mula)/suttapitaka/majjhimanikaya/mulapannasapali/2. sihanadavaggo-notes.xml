    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">adhippayāso</alternative>
      <alternative source-abbr="ka." source="cambodian">adhippāyo</alternative>
      <alternative source-abbr="sī." source="sri lanka">adhippāyo</alternative>
