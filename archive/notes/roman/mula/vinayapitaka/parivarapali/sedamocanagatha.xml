  <note reference-line="150" id="0">
    <original>no saṃghassa (ka.), no ca saṃghassa (syā.), no ce saṃghassa (sī.)</original>
    <type>no_match</type>
    <snippet>Mātaraṃ cīvaraṃ yāce, no ca saṅghe </snippet>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">no saṃghassa</alternative>
      <alternative source-abbr="syā." source="thai">no ca saṃghassa</alternative>
      <alternative source-abbr="sī." source="sri lanka">no ce saṃghassa</alternative>
    </alternatives>
  </note>
  <note reference-line="187" id="1">
    <original>na kāyikaṃ sunāsitaṃ (syā.)</original>
    <type>no_match</type>
    <snippet> kāyikañca garukaṃ, na kāyikaṃ na vācasikaṃ </snippet>
    <alternatives>
      <alternative source-abbr="syā." source="thai">na kāyikaṃ sunāsitaṃ</alternative>
    </alternatives>
  </note>
  <note reference-line="196" id="2">
    <original>viññūvibhāvitā (sī. syā.)</original>
    <type>auto</type>
    <snippet>Sedamocanikā gāthā, pañhā viññūhi vibhāvitāti </snippet>
    <match>vibhāvitāti</match>
    <alternatives>
