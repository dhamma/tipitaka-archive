<notes archive-path="/mula/suttapitaka/anguttaranikaya/ekakanipatapali/16.ekadhammapali">
  <note reference-line="13" id="0">
    <original>yenevaṃ (sī. syā. kaṃ. pī.)</original>
    <type>auto</type>
    <snippet>, bhikkhave, aññaṃ ekadhammampi samanupassāmi yena </snippet>
    <match>yena</match>
    <alternatives>
      <alternative source-abbr="sī." source="sri lanka">yenevaṃ</alternative>
