  <note line="3" original="sambodhapakkhikānaṃ (sī. syā. pī.)" reference-line="8" id="0">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">sambodhipakkhikānaṃ</alternative>
      <alternative source-abbr="sī." source="sri lanka">sambodhapakkhikānaṃ</alternative>
