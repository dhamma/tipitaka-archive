<notes archive-path="/mula/suttapitaka/anguttaranikaya/chakkanipatapali/7.devatavaggo">
  <note reference-line="6" id="0">
    <original>thīnaṃ (sī. syā. kaṃ. pī.)</original>
    <type>auto</type>
    <snippet> ‘‘Cha, bhikkhave, dhamme appahāya abhabbo arahattaṃ sacchikātuṃ. Katame cha? Thinaṃ </snippet>
    <match>Thinaṃ</match>
    <alternatives>
      <alternative source-abbr="sī." source="sri lanka">thīnaṃ</alternative>
      <alternative source-abbr="syā." source="thai">thīnaṃ</alternative>
      <alternative source-abbr="kaṃ." source="kam">thīnaṃ</alternative>
      <alternative source-abbr="pī." source="pali text society">thīnaṃ</alternative>
    </alternatives>
  </note>
  <note reference-line="9" id="1">
    <original>pāpake mitte (ka.)</original>
    <type>no_match</type>
    <snippet> vata, bhikkhave, bhikkhu pāpamitto pāpasahāyo pāpasampavaṅko, pāpamitte </snippet>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">pāpake mitte</alternative>
    </alternatives>
  </note>
  <note reference-line="20" id="2">
    <original>na paṭippassaddhaladdhena (sī.)</original>
    <type>no_match</type>
    <snippet> ‘‘‘So vata, bhikkhave, bhikkhu na santena samādhinā na paṇītena na paṭippassaddhiladdhena </snippet>
    <alternatives>
      <alternative source-abbr="sī." source="sri lanka">na paṭippassaddhaladdhena</alternative>
    </alternatives>
  </note>
  <note reference-line="26" id="3">
    <original>samādhimhi (ka.)</original>
    <type>auto</type>
    <snippet> ‘‘Chahi, bhikkhave, dhammehi samannāgato bhikkhu abhabbo samādhismiṃ </snippet>
    <match>samādhismiṃ</match>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">samādhimhi</alternative>
    </alternatives>
  </note>
  <note reference-line="26" id="4">
    <original>na samādhimhā (ka.) uparisattakanipāte devatāvagge pana sabbatthapi ‘‘samādhissa’’itveva dissati</original>
    <type>raw</type>
    <snippet> samādhissa samāpattikusalo hoti, na samādhissa ṭhitikusalo hoti, na samādhissa </snippet>
  </note>
  <note reference-line="34" id="5">
    <original>dutiyo (syā. ka.)</original>
    <type>auto</type>
    <snippet>Devatāvaggo sattamo. </snippet>
    <match>sattamo.</match>
    <alternatives>
