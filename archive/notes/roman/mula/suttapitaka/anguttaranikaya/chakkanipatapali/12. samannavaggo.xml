  <note reference-line="8" id="0">
    <original>tapasso (pī.) a. ni. 1.248</original>
    <type>auto</type>
    <snippet>, bhikkhave, dhammehi samannāgato tapusso </snippet>
    <match>tapusso</match>
    <alternatives>
      <alternative source-abbr="pī." source="pali text society">tapasso</alternative>
    </alternatives>
