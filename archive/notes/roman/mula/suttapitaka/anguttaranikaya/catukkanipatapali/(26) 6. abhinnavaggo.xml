<notes archive-path="/mula/suttapitaka/anguttaranikaya/catukkanipatapali/(26) 6. abhinnavaggo">
  <note line="3" original="pañcupādānakkhandhātissa vacanīyaṃ (ka.)" reference-line="4" id="0">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">Pañcupādānakkhandhā</alternative>
      <alternative source-abbr="ka." source="cambodian">pañcupādānakkhandhātissa vacanīyaṃ</alternative>
    </alternatives>
    <type>auto</type>
    <snippet>Katame ca, bhikkhave, dhammā abhiññā pariññeyyā? Pañcupādānakkhandhā </snippet>
  </note>
  <note line="12" original="piyavācā (ka.) a. ni. 4.32 passitabbaṃ" reference-line="12" id="1">
    <extra> a. ni. 4.32 passitabbaṃ</extra>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">piyavācā</alternative>
    </alternatives>
    <type>no_match</type>
    <snippet> Cattārimāni, bhikkhave, saṅgahavatthūni. Katamāni cattāri? Dānaṃ, peyyavajjaṃ </snippet>
  </note>
  <note line="20" original="māluṅkyaputto (sī. syā. kaṃ. pī.)" reference-line="14" id="2">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">mālukyaputto</alternative>
