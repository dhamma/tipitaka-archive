  <note line="74" original="bhikkhuṃsaṃgho (syā. pī.)" reference-line="37" id="7">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="syā." source="thai">bhikkhuṃsaṃgho</alternative>
      <alternative source-abbr="pī." source="pali text society">bhikkhuṃsaṃgho</alternative>
    </alternatives>
    <type>no_match</type>
