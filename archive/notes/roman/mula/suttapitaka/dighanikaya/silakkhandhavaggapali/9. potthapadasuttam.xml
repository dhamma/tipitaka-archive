<notes archive-path="/mula/suttapitaka/dighanikaya/silakkhandhavaggapali/9. potthapadasuttam">
  <note reference-line="8" id="0">
    <original>kāya nottha (syā. ka.)</original>
    <type>no_match</type>
    <snippet>Nisīdi bhagavā paññatte āsane. Poṭṭhapādopi kho paribbājako aññataraṃ nīcaṃ āsanaṃ gahetvā ekamantaṃ nisīdi. Ekamantaṃ nisinnaṃ kho poṭṭhapādaṃ paribbājakaṃ bhagavā etadavoca – ‘‘kāya nuttha </snippet>
    <alternatives>
      <alternative source-abbr="syā." source="thai">kāya nottha</alternative>
      <alternative source-abbr="ka." source="cambodian">kāya nottha</alternative>
    </alternatives>
  </note>
  <note reference-line="11" id="1">
    <original>na kho nāmetaṃ (sī. pī.)</original>
    <type>no_match</type>
    <snippet>‘‘Tamañño evamāha – ‘na kho pana metaṃ </snippet>
    <alternatives>
      <alternative source-abbr="sī." source="sri lanka">na kho nāmetaṃ</alternative>
      <alternative source-abbr="pī." source="pali text society">na kho nāmetaṃ</alternative>
    </alternatives>
  </note>
  <note reference-line="21" id="2">
    <original>purimasaññā (ka.)</original>
    <type>auto</type>
    <snippet> ākāso’ti ākāsānañcāyatanaṃ upasampajja viharati. Tassa yā purimā rūpasaññā </snippet>
    <match>rūpasaññā</match>
