  <note reference-line="26" id="4">
    <original>sattamehi (?)</original>
    <type>auto</type>
    <snippet> ācariyapācariyehi brahmā sakkhidiṭṭho. Napi kira tevijjānaṃ brāhmaṇānaṃ yāva sattamā </snippet>
    <match>sattamā</match>
    <alternatives>
      <alternative source-abbr="?" source="unknown">sattamehi</alternative>
