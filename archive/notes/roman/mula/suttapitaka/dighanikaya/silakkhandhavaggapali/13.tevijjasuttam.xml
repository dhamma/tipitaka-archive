<notes archive-path="/mula/suttapitaka/dighanikaya/silakkhandhavaggapali/13.tevijjasuttam">
  <note reference-line="16" id="0">
    <original>sattamācariyamahayugā (syā.)</original>
    <type>auto</type>
    <snippet>‘‘Kiṃ pana, vāseṭṭha, atthi koci tevijjānaṃ brāhmaṇānaṃ yāva sattamā ācariyāmahayugā </snippet>
    <match>ācariyāmahayugā</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">sattamācariyamahayugā</alternative>
    </alternatives>
  </note>
  <note reference-line="17" id="1">
    <original>samīhitaṃ (syā.)</original>
    <type>auto</type>
    <snippet> ‘‘Kiṃ pana, vāseṭṭha, yepi tevijjānaṃ brāhmaṇānaṃ pubbakā isayo mantānaṃ kattāro mantānaṃ pavattāro, yesamidaṃ etarahi tevijjā brāhmaṇā porāṇaṃ mantapadaṃ gītaṃ pavuttaṃ samihitaṃ </snippet>
    <match>samihitaṃ</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">samīhitaṃ</alternative>
    </alternatives>
  </note>
  <note reference-line="20" id="2">
    <original>teva (ka.)</original>
    <type>auto</type>
    <snippet>‘‘Sādhu, vāseṭṭha, te vata </snippet>
    <match>vata</match>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">teva</alternative>
    </alternatives>
  </note>
