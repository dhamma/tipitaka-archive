  <note line="224" original="cātummahārājikā (syā.)" reference-line="250" id="26">
    <extra></extra>
    <alternatives>
      <alternative source-abbr="vri" source="vipassana research institut">cātumahārājikā</alternative>
      <alternative source-abbr="syā." source="thai">cātummahārājikā</alternative>
    </alternatives>
