    <alternatives>
      <alternative source-abbr="syā." source="thai">avasesā</alternative>
      <alternative source-abbr="pī." source="pali text society">avasesā</alternative>
    </alternatives>
  </note>
  <note reference-line="248" id="1">
    <original>avaseso (syā.)</original>
    <type>auto</type>
    <snippet>Kāyāyatanaṃ ṭhapetvā avaseso na kāyāyatanaṃ, kāyo. Kāyañca kāyāyatanañca ṭhapetvā avasesaṃ </snippet>
    <match>avasesaṃ</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">avaseso</alternative>
    </alternatives>
  </note>
  <note reference-line="256" id="2">
    <original>avasesā (syā.)</original>
    <type>auto</type>
    <snippet>Sīlagandho samādhigandho paññāgandho na gandhāyatanaṃ, gandho. Gandhañca gandhāyatanañca ṭhapetvā avasesaṃ </snippet>
    <match>avasesaṃ</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">avasesā</alternative>
    </alternatives>
  </note>
