  <note reference-line="5" id="5">
    <original>kathalaṃ (ka.)</original>
    <type>auto</type>
    <snippet> masāragallaṃ tiṇaṃ kaṭṭhaṃ sakkharā kaṭhalaṃ </snippet>
    <match>kaṭhalaṃ</match>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">kathalaṃ</alternative>
