    <original>lohitaṅgo (syā. ka.), lohitako (?)</original>
    <type>auto</type>
    <snippet> muttā maṇi veḷuriyo saṅkho silā pavāḷaṃ rajataṃ jātarūpaṃ lohitaṅko </snippet>
    <match>lohitaṅko</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">lohitaṅgo</alternative>
