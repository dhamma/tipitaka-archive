<notes archive-path="/mula/abhidhammapitaka/patthanapali-1/1. kusalattikam">
  <note reference-line="1008" id="0">
    <original>āruppe (sabbattha)</original>
    <type>raw</type>
    <snippet> Kusalaṃ dhammaṃ saṃsaṭṭho kusalo dhammo uppajjati naadhipatipaccayā… napurejātapaccayā – arūpe </snippet>
  </note>
  <note reference-line="1247" id="1">
    <original>pahīnakilese (syā.)</original>
    <type>auto</type>
    <snippet> Akusalo dhammo kusalassa dhammassa ārammaṇapaccayena paccayo – sekkhā pahīne kilese </snippet>
    <match>kilese</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">pahīnakilese</alternative>
    </alternatives>
  </note>
  <note reference-line="1247" id="2">
    <original>vikkhambhitakilese (syā.)</original>
    <type>auto</type>
    <snippet> paccavekkhanti, vikkhambhite kilese </snippet>
    <match>kilese</match>
    <alternatives>
      <alternative source-abbr="syā." source="thai">vikkhambhitakilese</alternative>
    </alternatives>
  </note>
  <note reference-line="1362" id="3">
    <original>nānākhaṇikā (ka.)</original>
    <type>auto</type>
    <snippet>Kusalo dhammo abyākatassa dhammassa kammapaccayena paccayo – sahajātā, nānākkhaṇikā </snippet>
    <match>nānākkhaṇikā</match>
    <alternatives>
      <alternative source-abbr="ka." source="cambodian">nānākhaṇikā</alternative>
    </alternatives>
  </note>
  <note reference-line="2485" id="4">
