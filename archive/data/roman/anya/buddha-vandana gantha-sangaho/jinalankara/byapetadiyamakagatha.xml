    <hangnum line="2" number="110"></hangnum>
    <gatha1 line="3"> Ekantameva saparatthaparo mahesi,</gatha1>
    <gatha2 line="4">Ekantameva dasapāramitābalena;</gatha2>
    <gatha3 line="5">Ekantameva hatamārabalena tena,</gatha3>
